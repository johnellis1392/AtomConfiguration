# File containing custom snippets

'.source.js':
    'ES6 function syntax':
        'prefix': 'fes6'
        'body': """
        (${1:args}) => {
            ${2:body}
        }
        """

    'Blank ES6 test':
        'prefix': 'its'
        'body': """
        it("${1:description}", () => {
            ${2:body}
        })
        """

    'Pending Test':
        'prefix': 'itp'
        'body': """it("${1:description}")"""

    'Debugger statement':
        'prefix': 'de'
        'body': """
        debugger // eslint-disable-line
        """

# '.source.rust':
